{
  "name": "@syntek/ui",
  "version": "0.1.0",
  "private": false,
  "description": "A shared UI component library for React Web and React Native applications. Built with TypeScript, Tailwind CSS 4, and Nativewind 4.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook:web": "storybook dev -p 6006 -c .storybook-web",
    "storybook:web:build": "storybook build -c .storybook-web -o storybook-static-web",
    "storybook:native": "echo \"Note: React Native Storybook runs inside your RN app. Configure .ondevice/ in your React Native project.\""
  },
  "peerDependencies": {
    "nativewind": "^4.0.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "react-native": ">=0.70.0",
    "react-native-reanimated": ">=3.0.0",
    "react-native-safe-area-context": ">=4.0.0",
    "react-native-web": ">=0.18.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-reanimated": {
      "optional": true
    },
    "react-native-safe-area-context": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "autoprefixer": "^10.4.22",
    "babel-loader": "^10.0.0",
    "eslint": "^9.39.1",
    "nativewind": "^4.2.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native-web": "^0.21.2",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github-syntek:Syntek-Studio/ui_design_template.git"
  },
  "keywords": [
    "ui",
    "components",
    "react",
    "react-native",
    "tailwind",
    "nativewind",
    "design-system"
  ],
  "author": "Sam Bailey",
  "license": "ISC",
  "type": "module"
}
